{
	"presets": [
		[ "@babel/preset-env", {
			"modules": false,
			"targets": {
				"browsers": [
					"last 2 Chrome versions",
					"last 2 Firefox versions",
					"last 2 Safari versions"
				]
			}
		} ]
	],
	"plugins": [
		[ "@babel/plugin-transform-react-jsx", {
			"pragma": "wp.element.createElement"
		} ]
	]
}